<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Booking Schedule</title>
    <link rel="stylesheet" href="public/Booking.css">
</head>
<body>

<h1>Booking Schedule</h1>
<table>
    <thead>
        <tr>
            <th>#</th>
            <th>Machine Name</th>
            <th>Student Email</th>
            <th>Booking Start Time</th>
            <th>Booking End Time</th>
        </tr>
    </thead>
    <tbody>
        <% bookings.forEach((booking, index) => { %>
        <tr>
            <td><%= index + 1 %></td>
            <td><%= booking.machineId.machineName %></td>
            <td><%= booking.email %></td>
            <td><%= new Date(booking.setTime).toLocaleString() %></td>
            <td><%= new Date(booking.timeOverAt).toLocaleString() %></td>
        </tr>
        <% }) %>
    </tbody>
</table>

<div class="back-btns">
    <a href="/schedule/csv" class="btn">Export to CSV</a> <!-- Button for CSV export -->
</div>

</body>
</html>
